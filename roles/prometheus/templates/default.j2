server {
	listen 80 default_server;
        listen [::]:80 default_server;

	server_name _;

	location / {
		include uwsgi_params;
                #uwsgi_pass IP_address:Port
                uwsgi_pass unix:/opt/agama/agama.sock;
	}

        location /metrics {
		proxy_pass http://vm1.{{ domain_name }}:9100/metrics;
	}

        location /prometheus {
                proxy_pass http://vm1.{{ domain_name }}:9090;
       }
}
