$TTL	604800
dystopia.net.	IN	SOA	core.dystopia.net. master.dystopia.net. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

dystopia.net.	IN	NS	core.dystopia.net.
satellite	IN	A	{{ hostvars['my_vm']['ansible_default_ipv4']['address'] }}
core		IN	A	{{ hostvars['my_vm2']['ansible_default_ipv4']['address'] }}
